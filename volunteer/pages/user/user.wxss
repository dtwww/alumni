.container {
  padding-bottom: 100rpx;
}

.head-view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-bottom: 30rpx;
}

.head {
  margin: 60rpx 0 30rpx 0;
  text-align: center;
  align-self: center;
  color: #07C160;
  font-size: 45rpx;
}

.line-view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.line {
  width: 94%;
  height: 5rpx;
  background-color: #d6d6d6;
}

.content {
  width: 100%;
  height: 100%;
}

.piece {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 35rpx;
  padding: 30rpx 40rpx 30rpx 40rpx;
  margin: 20rpx 0 15rpx 0;
  align-items: center;
  color: #5b5b5b;
}

.text2 {
  font-weight: 600;
}